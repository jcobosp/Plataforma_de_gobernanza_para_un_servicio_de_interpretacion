{
  "name": "blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "super": "supervisor ./bin/www",
    "migrate": "sequelize db:migrate --url sqlite://$(pwd)/blog.sqlite",
    "seed": "sequelize db:seed:all --url sqlite://$(pwd)/blog.sqlite",
    "migrate_win": "sequelize db:migrate --url sqlite://%cd%/blog.sqlite",
    "seed_win": "sequelize db:seed:all --url sqlite://%cd%/blog.sqlite",
    "seed_teams_undo": "sequelize db:seed:undo --seed 20240212125002-FillTeamsTable"
  },
  "dependencies": {
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.7",
    "debug": "~4.4.0",
    "ejs": "~3.1.10",
    "express": "~4.21.2",
    "express-generator": "^4.16.1",
    "express-partials": "^0.3.0",
    "express-session": "^1.18.1",
    "http-errors": "~2.0.0",
    "method-override": "^3.0.0", 
    "morgan": "~1.10.0",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^3.0.3",
    "sequelize": "^6.37.6",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.1.7",
    "supervisor": "^0.12.0"
  }
}
