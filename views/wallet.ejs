      <section>
        <h2>Wallet de Usuarios</h2>
        <ul>
          <% users.forEach(user => { %>
            <li>
              <%= user.username %>: <%= user.wallet %>
              <form method="post" action="/wallet/<%= user.userId %>/<%= user.teamId %>/addPointWallet">
                <button type="submit">Sumar Punto</button>
              </form>
              <form method="post" action="/wallet/<%= user.userId %>/<%= user.teamId %>/removePointWallet">
                <button type="submit" <%= user.wallet === 0 ? 'disabled' : '' %>>Restar Punto</button>
              </form>
            </li>
          <% }); %>
        </ul>
      </section>



